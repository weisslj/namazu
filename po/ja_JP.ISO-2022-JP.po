"    -C, --show-config       \033\044\102\100\137\104\152\044\162\111\075\074\050\044\071\044\153\033\050\102\n"
"    -q, --quiet             \033\044\102\070\041\072\167\067\153\062\114\060\112\063\060\044\116%%\141%%\103%%\073\041\074%%\070\044\162\111\075\074\050\044\067\044\112\044\044\033\050\102\n"
"    -L, --lang=lang         \033\044\102%%\141%%\103%%\073\041\074%%\070\044\116\070\100\070\154\044\162\100\137\104\152\044\071\044\153\033\050\102 ja \033\044\102\044\136\044\077\044\117\033\050\102 en\n"
"    -v, --version           \033\044\102%%\164%%\041\041\074%%\070%%\147%%\163\044\162\111\075\074\050\044\071\044\153\033\050\102\n"
